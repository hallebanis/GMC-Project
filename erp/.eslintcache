Filter(e.target.value)}\r\n          value={nameFilter}\r\n        />\r\n        <lable>Role</lable>\r\n        <RoleDropDown\r\n          setRoleTitle={setRoleFilter}\r\n          dropDownMsg={roleFilter ? roleFilter : \"Select a role from the list\"}\r\n        />\r\n      </Form>\r\n      <Button onClick={handleReset}>Reset</Button>\r\n      <UserList\r\n        userList={\r\n          users\r\n            ? users.users.filter((elm) =>\r\n                elm.personnelId.nom\r\n                  ? (elm.personnelId.nom\r\n                      .toLowerCase()\r\n                      .includes(nameFilter.toLowerCase()) ||\r\n                      elm.personnelId.prenom\r\n                        .toLowerCase()\r\n                        .includes(nameFilter.toLowerCase())) &&\r\n                    (roleFilter === \"Select a role from the list\"\r\n                      ? true\r\n                      : elm.role.titre.includes(roleFilter))\r\n                  : null\r\n              )\r\n            : null\r\n        }\r\n      />\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default UsersListPage;\r\n","D:\\GMC\\Project ERP\\erp\\src\\components\\admin\\UserList.js",[],"D:\\GMC\\Project ERP\\erp\\src\\components\\admin\\User.js",[],"D:\\GMC\\Project ERP\\erp\\src\\actions\\admin\\usersActions.js",[],"D:\\GMC\\Project ERP\\erp\\src\\pages\\admin\\UserDetailsPage.js",["144"],"import React, { useEffect, useState } from \"react\";\r\nimport { useSelector } from \"react-redux\";\r\nimport User from \"../../components/admin/User\";\r\n\r\nconst UserDetailsPage = ({ match, history }) => {\r\n  const users = useSelector((state) => state.users);\r\n  const [changeMaid, setChangeMaid] = useState(\"false\");\r\n  const user = users.users.filter((elm) => elm._id === match.params.id);\r\n  useEffect(() => {\r\n    if (changeMaid === true) history.push(\"/admin-dashboard/users\");\r\n  }, [changeMaid]);\r\n  return (\r\n    <div>\r\n      <h1>user</h1>\r\n      <h2>{}</h2>\r\n     